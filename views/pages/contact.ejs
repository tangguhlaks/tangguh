<div class="container text-white">
   <div class="text-center mb-5">
        <h3 style="color:#66FD0F;" class="fw-bold">Let’s Collaborate</h3>
        <h3 class="display-7 fw-semibold mt-3">
            Have a project in mind or need guidance? Let’s connect.
        </h3>
    </div>
    <hr>
    <br>
    <div class="row justify-content-center">
        <div class="col-lg-10">
            <form class="p-4 rounded" style="border:1px solid; background: transparent;">
                <div class="row g-3">
                    <div class="col-md-6">
                        <input type="text" class="form-control text-white bg-white bg-opacity-10 border-0 shadow-none w-100 ps-3 py-2" placeholder="Full Name*" required style="border:1px solid;">
                    </div>
                    <div class="col-md-6">
                        <input type="email" class="form-control text-white bg-white bg-opacity-10 border-0 shadow-none w-100 ps-3 py-2" placeholder="Email*" required style="border:1px solid;">
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="form-control text-white bg-white bg-opacity-10 border-0 shadow-none w-100 ps-3 py-2" placeholder="Phone" style="border:1px solid;">
                    </div>
                    <div class="col-md-6">
                        <input type="text" class="form-control text-white bg-white bg-opacity-10 border-0 shadow-none w-100 ps-3 py-2" placeholder="Subject" style="border:1px solid;">
                    </div>
                    <div class="col-12">
                        <textarea rows="4" class="form-control text-white bg-white bg-opacity-10 border-0 shadow-none w-100 ps-3 py-2" placeholder="Message" style="border:1px solid;"></textarea>
                    </div>
                    <div class="col-12 text-center mt-3">
                        <a href="#" class="btn btn-outline-primary mt-4 position-relative pe-5"
                            data-aos="fade-up" data-aos-duration="1600">
                            Send Message
                            <div class="position-absolute top-50 end-0 translate-middle-y me-3">
                                <svg class="arrow-right p-1" width="28" height="28">
                                    <use xlink:href="#arrow-right"></use>
                                </svg>
                            </div>
                        </a>
                    </div>
                </div>
            </form>

            <!-- Quick Connect -->
            <div class="text-center mt-5">
                <h5 style="color:#66FD0F;">Quick Connect</h5>
                <div class="d-flex justify-content-center gap-4 mt-3">
                    <!-- GitHub -->
                    <a href="https://github.com/yourusername" target="_blank" class="text-white fs-4">
                        <i class="bi bi-github"></i>
                    </a>
                    <!-- LinkedIn -->
                    <a href="https://linkedin.com/in/yourusername" target="_blank" class="text-white fs-4">
                        <i class="bi bi-linkedin"></i>
                    </a>
                    <!-- Instagram -->
                    <a href="https://instagram.com/yourusername" target="_blank" class="text-white fs-4">
                        <i class="bi bi-instagram"></i>
                    </a>
                    <!-- WhatsApp -->
                    <a href="https://wa.me/6281234567890" target="_blank" class="text-white fs-4">
                        <i class="bi bi-whatsapp"></i>
                    </a>
                    <!-- Email -->
                    <a href="mailto:youremail@example.com" target="_blank" class="text-white fs-4">
                        <i class="bi bi-envelope"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <br>
    <hr>
</div>
